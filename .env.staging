# Staging Environment Configuration
IOT_ENV=staging

# Claude API
ANTHROPIC_API_KEY=your_staging_api_key_here

# Staging can use either SQLite or Oracle
STAGING_DB_TYPE=sqlite  # or 'oracle'

# Oracle Staging Database Configuration (if using Oracle)
ORACLE_STAGING_HOST=staging-oracle.company.com
ORACLE_STAGING_PORT=1521
ORACLE_STAGING_SERVICE=STAGING
ORACLE_STAGING_USER=iot_staging
ORACLE_STAGING_PASSWORD=staging_password
ORACLE_STAGING_SCHEMA=IOT_STAGING